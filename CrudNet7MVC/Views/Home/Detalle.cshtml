@model CrudNet7MVC.Models.Contacto

@{
    ViewData["Title"] = "Detalle Contacto";
}


<h1 class="text-center">Contacto con .NET 7</h1>


<div class="row">
    <div class="offset-12">
        <div class="card border">
            <div class="card-header bg-dark">
                <h3 class="text-white">
                    <strong>Detalle de usuario</strong>
                </h3>
            </div>
            <div class="card-body bg-light">
                <ul class="list-group">
                    <li class="list-group-item">
                        <strong>Nombre: </strong>@Html.DisplayFor(m => m.Nombre)
                    </li>
                    <li class="list-group-item">
                        <strong>Telefono: </strong>@Html.DisplayFor(m => m.Telefono)
                    </li>
                    <li class="list-group-item"> 
                        <strong>Celular: </strong>@Html.DisplayFor(m => m.Celular)
                    </li>
                    <li class="list-group-item">
                        <strong>Email: </strong>@Html.DisplayFor(m => m.Email)
                    </li>
                </ul>
                <a asp-controller="Home" asp-action="index">Volver</a>
            </div>
        </div>
    </div>
</div>
